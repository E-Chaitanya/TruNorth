# ===== Global =====
FRONTEND_URL=https://trunorth-nine.vercel.app
BACKEND_URL=https://trunorth-backend.onrender.com

# ===== PostgreSQL (AWS RDS) =====
DATABASE_URL=postgresql://<username>:<password>@<endpoint>:5432/<dbname>

# ===== Pinecone =====
PINECONE_API_KEY=<your-pinecone-api-key>
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=trunorth-index

# ===== Firebase Auth =====
FIREBASE_API_KEY=<your-firebase-api-key>
FIREBASE_AUTH_DOMAIN=trunorth-auth.firebaseapp.com
FIREBASE_PROJECT_ID=trunorth-auth
FIREBASE_STORAGE_BUCKET=trunorth-auth.appspot.com
FIREBASE_MESSAGING_SENDER_ID=<your-messaging-sender-id>
FIREBASE_APP_ID=<your-app-id>

# ===== Optional (for later phases) =====
FIREBASE_MEASUREMENT_ID=<optional>
